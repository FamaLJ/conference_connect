<div class="page-header">
  <h1>
    Attendees in Your Sessions
  </h1>
</div>

<div class="row">
  <div class="col-md-12 padding-inside-bottom-lg">
    <table class="table table-striped table-hover">
      <tr>
        <th>User ID</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Company Name</th>
        <th>LinkedIn Url</th>
      </tr>
      <% @users.each do |user| %>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.email %></td>
        <td><%= user.last_name %></td>
        <td><%= user.company_name %></td>
        <td>
          <img src="<%= user.linked_in_url %>"
        </td>
        <td>
          <a href="<%= user.linked_in_url %>" class="btn btn-primary">View LinkedIn</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>




