<div class="page-header">
  <h1>
    Registered Attendees By Session
  </h1>
</div>

<div class="row padding-inside-top-sm padding-inside-bottom-md">
  <div class="col-md-12">
    <h4> Breakout Sessions A: Breaking New Ground  </h4>
  </div>
</div>


<div class="row">
  <div class="col-md-12 padding-inside-bottom-sm ">
    <table class="table table-striped table-hover">
      <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Company</th>
        <th>Subject Matter Interest</th>
        <th>Industry Interest</th>
        <th>Job Inquiry</th>
        <th>Other</th>
        <th></th>
      </tr>
      <% @enrollments.where(:breakoutenroll_id => 1).each do |enrollment| %>
      <tr>
        <td><%= User.find_by(:id =>enrollment.userenroll_id).first_name %></td>
        <td><%= User.find_by(:id =>enrollment.userenroll_id).last_name %></td>
        <td><%= User.find_by(:id =>enrollment.userenroll_id).company_name %></td>
        <td><%= enrollment.content %></td>
        <td><%= enrollment.industry %></td>
        <td><%= enrollment.job %></td>
        <td><%= enrollment.other %></td>
        <td>
          <a href="<%= User.find_by(:id =>enrollment.userenroll_id).linked_in_url %>" class="btn btn-primary">View LinkedIn</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>






