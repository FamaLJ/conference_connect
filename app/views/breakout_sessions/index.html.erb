<div class="page-header">
  <h1>
    Breakout sessions
    <a href="/breakout_sessions/new" class="btn btn-lg btn-success">New Breakout session</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Breakout session name</th>
        <th>Description</th>
        <th>Speaker</th>
        <th>Starts at</th>
        <th>Ends at</th>
        <th>Actions</th>
      </tr>

      <% @breakout_sessions.each do |breakout_session| %>
      <tr>
        <td><%= breakout_session.breakout_session_name %></td>
        <td><%= breakout_session.description %></td>
        <td><%= breakout_session.speaker %></td>
        <td><%= breakout_session.starts_at %></td>
        <td><%= breakout_session.ends_at %></td>


        <td>
          <a href="/breakout_sessions/<%= breakout_session.id %>" class="btn btn-primary">Show</a>
          <a href="/breakout_sessions/<%= breakout_session.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_breakout_session/<%= breakout_session.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
