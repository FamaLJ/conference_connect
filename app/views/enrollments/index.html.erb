<div class="page-header">
  <h1>
    Enrollments
    <a href="/enrollments/new" class="btn btn-lg btn-success">New Enrollment</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Other</th>
        <th>Industry</th>
        <th>Job</th>
        <th>Content</th>
        <th>Userenroll</th>
        <th>Breakoutenroll</th>
        <th>Actions</th>
      </tr>

      <% @enrollments.each do |enrollment| %>
      <tr>
        <td><%= enrollment.other %></td>
        <td><%= enrollment.industry %></td>
        <td><%= enrollment.job %></td>
        <td><%= enrollment.content %></td>
        <td><%= enrollment.userenroll_id %></td>
        <td><%= enrollment.breakoutenroll_id %></td>
        <td>
          <a href="/enrollments/<%= enrollment.id %>" class="btn btn-primary">Show</a>
          <a href="/enrollments/<%= enrollment.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_enrollment/<%= enrollment.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
