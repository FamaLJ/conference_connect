<div class="page-header">
  <h1>
    My Registered Sessions
    <a href="/enrollments/new" class="btn btn-lg btn-success">New Enrollment</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12 padding-inside-bottom-lg">
    <table class="table table-striped table-hover">
      <tr>
        <th>Breakout Name</th>
        <th>Interested in Content</th>
        <th>Interested in Industry</th>
        <th>Looking for Jobs in this field</th>
        <th>Other</th>
          <th>Actions</th>


      </tr>

      <% @enrollments.each do |enrollment| %>
      <tr>
        <td><%= Breakoutsession.find_by(:id =>enrollment.breakoutenroll_id).breakoutname %></td>
        <td><%= enrollment.content %></td>
        <td><%= enrollment.industry %></td>
        <td><%= enrollment.job %></td>
        <td><%= enrollment.other %></td>

        <td>
          <a href="/enrollments/<%= enrollment.id %>" class="btn btn-primary">Show</a>
          <a href="/enrollments/<%= enrollment.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_enrollment/<%= enrollment.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
