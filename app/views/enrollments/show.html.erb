<div class="page-header">
  <h3>Enrollment #<%= @enrollment.id %></h3>
</div>

<div class="row">
  <div class="col-md-12">
    <dl class="dl-horizontal">
      <dt>Other</dt>
      <dd><%= @enrollment.other %></dd>

      <dt>Industry interest</dt>
      <dd><%= @enrollment.industry_interest %></dd>

      <dt>Job opportunities</dt>
      <dd><%= @enrollment.job_opportunities %></dd>

      <dt>Speaker connection</dt>
      <dd><%= @enrollment.speaker_connection %></dd>

      <dt>User</dt>
      <dd><%= @enrollment.user_id %></dd>

      <dt>Breakout session</dt>
      <dd><%= @enrollment.breakout_session_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/enrollments" class="btn btn-primary">
        Back
      </a>
      <a href="/enrollments/<%= @enrollment.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_enrollment/<%= @enrollment.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
