      </tr>

      <% @breakoutsessions.each do |breakoutsession| %>
      <tr>
        
        <td><%= breakoutsession.breakoutname %></td>
        <td><%= breakoutsession.description %></td>
        <!-- <td><%= breakoutsession.starts_at %></td> -->
