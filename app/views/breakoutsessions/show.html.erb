<div class="page-header">
  <h3>Breakoutsession #<%= @breakoutsession.id %></h3>
</div>

<div class="row">
  <div class="col-md-12">
    <dl class="dl-horizontal">
      <dt>Breakout</dt>
      <dd><%= @breakoutsession.breakout %></dd>

      <dt>Description</dt>
      <dd><%= @breakoutsession.description %></dd>

      <dt>Ends at</dt>
      <dd><%= @breakoutsession.ends_at %></dd>

      <dt>Starts at</dt>
      <dd><%= @breakoutsession.starts_at %></dd>

      <dt>Speaker</dt>
      <dd><%= @breakoutsession.speaker %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/breakoutsessions" class="btn btn-primary">
        Back
      </a>
      <a href="/breakoutsessions/<%= @breakoutsession.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_breakoutsession/<%= @breakoutsession.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
